<p style='color:red'><%= flash.try(:alert) %></p>
<h1>
  
  <% if @artist %>
    <%= @artist.name%>'s Songs
  <% else %>
    List of All Songs
  <% end %>
</h1>
<ul>
<% @songs.each do |song| %>
  <li><%= display_artist(song) %> - <%= link_to song.title, song_path(song) %></li>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
